package com.andre.array;

public class Treino {

	public static void main(String[] args) {
		int [][] matriz = new int [2][2]; 
		
		matriz[0][0]=2;
		matriz[0][1]=5;
		
		matriz[1][0]=8;
		matriz[1][1]=2;
		
		//int maior = 0;
		
		for(int i =0;i < matriz.length; i++)
			for(int j =0; j < matriz.length; j++){
				
				System.out.print(matriz[i][j] + "-");
			}
		
		
	}

}
