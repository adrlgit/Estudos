package com.andre.array;

public class SomarArrays {

	public static void main(String[] args) {
		int[] array = { 10, 10, 10, 100, 0, 2, 2, 2, 2};
		int total = 0;
		
		//adicionar valor de cada evento ao total
		for(int num: array)
			total += num;
		
		System.out.printf("Total: %d\n",total);
	}

}
